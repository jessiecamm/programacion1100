Algoritmo sumar_numeros
    
    Definir num1, num2, resultado Como entero;
   
    Escribir "Ingrese el primer número";
    Leer num1;
    Escribir "Ingrese el segundo número";
    Leer num2;
	
    resultado=(num1+num2);
	Escribir "La suma de los números ingresados es: " , resultado;	
	
FinAlgoritmo
